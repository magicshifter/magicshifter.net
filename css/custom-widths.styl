@media (max-width: 900px)
	header.main 
		img#logo,
		a,
		div.desc
			font-size 0.9em

	#images ul li
		width 30%
		margin 1%

@media (max-width: 800px)
	header.main
		img#logo,
		a,
		div.desc
			width 100%
			margin 0
			float none

@media (max-width: 730px)

	a.btn
		display inline-block
		margin 0 0 1em

@media (max-width: 400px)
	#images ul li
		width 45%
		margin 1%

	header.main 
		img#logo,
		a,
		div.desc
			float none
			width 100%
			margin 0

		a
			margin 1em 0 0

	table
		font-size 0.8em

	footer.main #aws a img
		display block
		margin 0 auto